{
  "name": "ioguimaraes/ready-keepass-php",
  "type": "library",
  "description": "A library for reading KeePass 2.x databases",
  "license": "MIT",
  "keywords": [
    "keepass",
    "password",
    "manager",
    "security",
    "encryption",
    "decryption",
    "openssl",
    "cipher",
    "cryptography",
    "decryptography",
    "crypto",
    "encrypt",
    "decrypt"
  ],
  "authors": [
    {
      "name": "Igor de Oliveira GuimarÃ£es",
      "email": "ig.oliveragui@gmail.com",
      "homepage": "https://ioguimaraes.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "KeePassPHP\\": "src"
    }
  },
  "require": {
    "php": ">=5.3.0",
    "ext-hash": "*",
    "ext-json": "*",
    "ext-xmlreader": "*",
    "ext-zlib": "*",
    "ext-libxml": "*",
    "ext-dom": "*",
    "ext-simplexml": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "suggest": {
    "ext-mcrypt": "An alternative to the preferred OpenSSL encryption",
    "ext-openssl": "A better and faster alternative to mcrypt"
  }
}
